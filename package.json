{
	"name": "sskv",
	"version": "0.0.1",
	"description": "Super simple key value with type safety and implement your own load and save function.",
	"main": "dist/main.js",
	"module": "dist/main.m.js",
	"types": "dist/main.d.ts",
	"scripts": {
		"lint": "tsc",
		"build": "tsup main.ts --format cjs,esm --dts",
		"format:check": "prettier --check --ignore-path .prettierignore .",
		"format:write": "prettier --write --ignore-path .prettierignore ."
	},
	"keywords": [
		"key",
		"value",
		"database",
		"config"
	],
	"author": "Anas Rin <mynameanasrar@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/anasrar/sskv.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.41.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	}
}
